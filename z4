N = int(input("Введите количество чисел: "))
if N < 10:
    print("Данное количество чисел не удовлетворяет условию. Повторите попытку")
    N = int(input("Введите повторно количество чисел: "))
mass = input("Введите числа через пробел: ")
prob = mass.split(" ")
k = len(prob)
while k != N:
    print ('Вы Ввели неверное количество чисел, повторите попытку')
    mass = input("Введите числа через пробел: ")
    prob = mass.split(" ")
    k = len(prob)
del prob[3:7]
dob = input("Введите 2 новых значения через пробел ")
new = dob.split(' ')
rez = prob + new
print(rez)
